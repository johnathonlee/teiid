<!-- !!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!! --> 
<!-- Note: This property file is to identify the following metamodel URIs attributes:    -->
<!-- 	1. entitled level (all, model, group, and element)  							 -->
<!--   2. is it an xml document (true | false)											 -->

<!--   These attributes are comma seperated and ordered									 -->
<!-- !!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!! -->

<urimodeltypes>
	<uri name="http://www.metamatrix.com/metamodels/Relational" authlevel="all" xmldoctype="false" physicalbindingallowed="true"/>
	<uri name="http://www.metamatrix.com/metamodels/DataAccess" authlevel="all" xmldoctype="false" physicalbindingallowed="true"/>
	<uri name="http://www.metamatrix.com/metamodels/XmlDocument" authlevel="group" xmldoctype="true" physicalbindingallowed="false"/>
    <uri name="http://www.metamatrix.com/metamodels/XmlService" authlevel="all" xmldoctype="true" physicalbindingallowed="true"/>
</urimodeltypes>
